package br.com.lotto.dao;

import java.util.List;

import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.data.jpa.repository.Query;
import org.springframework.data.repository.query.Param;
import org.springframework.stereotype.Repository;

import br.com.lotto.entity.Frequencia;
import br.com.lotto.entity.Megasena;

@Repository
public interface MegaSenaRepository extends JpaRepository<Megasena,Integer>{

	@Query(value = "select  numeroidnumero as numero, count(*) as frequencia from megasenanumero group by numeroidnumero order by frequencia desc")
	public List<Frequencia> getFrequancia();
}
